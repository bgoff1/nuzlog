{
  "name": "nuzlog",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "tsc && vite build",
    "db:types": "kysely-codegen --dialect sqlite --url public/pokemon.db --out-file src/database/types.gen.ts",
    "deploy:netlify": "netlify deploy --prod",
    "dev": "vite",
    "format:check": "prettier . --check",
    "lint": "eslint src",
    "prepare": "husky install",
    "preview": "vite preview",
    "routes": "tsc -p scripts/tsconfig.json && node scripts/dist && rm -rf scripts/dist",
    "test": "vitest run"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx,json,md,js,jsx}": "prettier --write"
  },
  "prettier": {
    "bracketSameLine": true,
    "plugins": [
      "prettier-plugin-packagejson",
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss"
    ]
  },
  "dependencies": {
    "@solidjs/router": "^0.10.8",
    "clsx": "^2.1.0",
    "comlink": "^4.4.1",
    "idb-keyval": "^6.2.1",
    "kysely": "^0.27.2",
    "solid-icons": "^1.1.0",
    "solid-js": "^1.8.7",
    "solid-toast": "^0.5.0",
    "sql.js": "^1.10.2"
  },
  "devDependencies": {
    "@bgoff1/pokeapi-types": "^1.0.3",
    "@catppuccin/palette": "^1.0.3",
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@solidjs/testing-library": "^0.8.6",
    "@testing-library/jest-dom": "^6.4.0",
    "@types/node": "^20.11.0",
    "@types/sql.js": "^1.4.9",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@vitest/coverage-v8": "^1.2.2",
    "autoprefixer": "^10.4.16",
    "better-sqlite3": "^9.3.0",
    "cssnano": "^6.0.3",
    "daisyui": "^4.6.0",
    "eslint": "^8.56.0",
    "eslint-plugin-solid": "^0.13.1",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "husky": "^8.0.3",
    "jsdom": "^24.0.0",
    "kysely-codegen": "^0.11.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.4.9",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-pwa": "^0.17.4",
    "vite-plugin-solid": "^2.8.0",
    "vitest": "^1.2.2",
    "workbox-core": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-window": "^7.0.0"
  },
  "packageManager": "pnpm@8.14.1",
  "volta": {
    "node": "20.11.0"
  }
}
